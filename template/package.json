    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.2",
    {{#e2e}}
    "testcafe": "^0.18.6",
    "testcafe-vue-selectors": "^2.1.0",
    "globby": "^8.0.0",
    {{/e2e}}
    "ts-loader": "^3.2.0",
    "typescript": "~2.7.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.0.2",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.0",
    "webpack-merge": "^4.1.1"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
