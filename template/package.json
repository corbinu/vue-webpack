{
  "name": "{{ name }}",
  "version": "0.1.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    {{#e2e}}
    "dev:test": "webpack-dev-server --config build/webpack.prod.conf.js",
    {{/e2e}}
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    {{#e2e}}
    "e2e": "node test/e2e/runner.js",
    {{/e2e}}
    "test": "npm run lint{{#unit}} && npm run unit{{/unit}}{{#e2e}} && npm run e2e{{/e2e}}",
    "lint": "eslint --ext .js,.ts,.vue src{{#if_or unit e2e}} test{{/if_or}} config build",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue src{{#if_or unit e2e}} test{{/if_or}} config build",
    "build": "node build/build.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.39",
    "axios": "^0.18.0",
    "vue": "^2.5.13"{{#router}},
    "vue-router": "^3.0.1"{{/router}}
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-stage-3": "^7.0.0-beta.39",
    "@babel/runtime": "^7.0.0-beta.39",
    "@corbinu/eslint-plugin-corbinu": "^3.13.1",
    "autoprefixer": "^8.0.0",
    "babel-loader": "^8.0.0-beta.0",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.15.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    {{#unit}}
    "@types/jest": "^22.0.1",
    "jest": "^22.1.2",
    "jest-serializer-vue": "^0.3.0",
    "ts-jest": "^22.0.1",
    "vue-jest": "^2.0.0",
    "@vue/test-utils": "^1.0.0-beta.10",
    "vue-server-renderer": "^2.5.13",
    {{/unit}}
    "optimize-css-assets-webpack-plugin": "^3.2.0",
